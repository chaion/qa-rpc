{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id":"eth_call.request.json",
    "title": "eth_call request parameters",
    "description": "Executes a new message call immediately without creating a transaction on the block chain.",


    "additionalItems": false,
    "items": [
      {
        "$ref":"object.json#/definitions/TRANSACTION_CALL",
        "required":["to"]
      },
      {
        "oneOf":[
          {"$ref":"type.json#/definitions/TAG"},
          {"$ref":"type.json#/definitions/QUANTITY"}
        ],
        "description":"integer block number, or the string \"latest\", \"earliest\" or \"pending\", see the default block parameter"
      }
    ],
    "minItems": 1,
    "maxItems": 2,

    "examples": [
      [
        {
          "from": "0xa03a4a3ccff2b2f0f6ba368f2668126ee01e73b6b44faf801135a2d4183edb2c",
          "to": "0xa0f40004d87d2385248603022988b2ed1e9f463b1f7bc8187515984402e4934b",
          "gas": "0xd431",
          "data": "0xa87d942c"
        }
      ],
      [
        {
          "to": "0xa0f40004d87d2385248603022988b2ed1e9f463b1f7bc8187515984402e4934b",
          "gasPrice": "0xd431",
          "data": "0xa87d942c"
        },
        "0x0"
      ],
      [
        {
          "to": "0xa0f40004d87d2385248603022988b2ed1e9f463b1f7bc8187515984402e4934b",
          "gasPrice": "0xd431",
          "data": "0xa87d942c"
        },
        "pending"
      ]
	]
}
